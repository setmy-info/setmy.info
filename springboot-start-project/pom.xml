            </properties>
        </profile>
        <profile>
            <id>prelive</id>
            <properties>
                <spring-boot.run.profiles>default,prelive</spring-boot.run.profiles>
            </properties>
        </profile>
        <profile>
            <id>ci</id>
            <properties>
                <spring-boot.run.profiles>default,ci</spring-boot.run.profiles>
            </properties>
        </profile>
        <profile>
            <id>live</id>
            <properties>
                <spring-boot.run.profiles>default,live</spring-boot.run.profiles>
            </properties>
        </profile>
    </profiles>

