<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD GlassFish Application Server 3.0 Servlet 3.0//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_3_0-0.dtd">
<sun-web-app error-url="">
    <context-root>/pub-system</context-root>
  <!--ejb-ref>
    <ejb-ref-name>SysContextBean</ejb-ref-name>
    <jndi-name>ejb/SysContextBeanJNDI</jndi-name>
  </ejb-ref-->
    <class-loader delegate="true"/>
    <jsp-config>
        <property name="classdebuginfo" value="true">
            <description>GF Base J2EE Platform</description>
        </property>
        <property name="mappedfile" value="true">
            <description>Maintain a one-to-one correspondence between static content and the generated servlet class' java code</description>
        </property>
        <property name="keepgenerated" value="true">
            <description>Keep a copy of the generated servlet class' java code.</description>
        </property>
    </jsp-config>
</sun-web-app>
