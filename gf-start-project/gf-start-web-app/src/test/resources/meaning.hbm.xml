<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>	

	<class name="ee.home.lib.word.Meaning" table="meaning">
		<id name="id" type="java.lang.Long" column="id">
  	  	  	<generator class="sequence">
  	  	  		<param name="sequence">meaning_id_seq</param>
  	  	  	</generator>
  	  	</id>
  	  	<property name="translation" column="translation" type="java.lang.String"/>
  	  	<many-to-one name="" column="" class=""/>
  	</class>
  	
</hibernate-mapping> 