
DOCKER_FOLDERS=docker-centos docker-centos-java docker-centos-java-tomcat docker-centos-nginx docker-centos-node

all: $(DOCKER_FOLDERS)

docker-centos:
	@cd $(@); pwd; echo $(MAKE)

docker-centos-java:
	@cd $(@); pwd; echo $(MAKE)

docker-centos-java-tomcat:
	@cd $(@); pwd; echo $(MAKE)

docker-centos-nginx:
	@cd $(@); pwd; echo $(MAKE)

docker-centos-node:
	@cd $(@); pwd; echo $(MAKE)

.PHONY: all $(DOCKER_FOLDERS)
